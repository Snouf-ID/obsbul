<?xml version="1.0" standalone=yes>
<entity>
    <name>Sphere</name>
    <desc>A sphere using albedo and normal textures with phong shading</desc>
    <gfx>
        <textures>
            <albedo>skybox</albedo>
        </textures>

        <model>skybox</model>

   <!-- Shader defines separated with comma -->
        <shader_defines>
            ALBEDO_TEX,
            FRUSTUM,
            SKYBOX
        </shader_defines>
    </gfx>
</entity>
